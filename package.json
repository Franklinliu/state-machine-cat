{
  "name": "state-machine-cat",
  "version": "2.1.2",
  "description": "write beautiful state charts",
  "main": "src/index.js",
  "scripts": {
    "depcruise": "depcruise -v -X node_modules -- src test",
    "depcruise:graph": "depcruise -M cjs -T dot -v -X node_modules -- bin/smcat | dot -T svg > tmp_deps.svg",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "npm-check-updates": "ncu --upgrade",
    "nsp": "nsp check",
    "preversion": "echo \"please manualy bump version in package.json & index.js\" && exit 1",
    "test": "mocha -R spec --recursive test",
    "test:cover": "istanbul cover node_modules/mocha/bin/_mocha -- --timeout 4000 --recursive test"
  },
  "keywords": [
    "state",
    "state chart",
    "state diagram",
    "state machine",
    "finite state machine",
    "fsm"
  ],
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "devDependencies": {
    "chai": "4.1.2",
    "chai-xml": "0.3.1",
    "dependency-cruiser": "2.7.2",
    "eslint": "4.11.0",
    "handlebars": "4.0.11",
    "istanbul": "0.4.5",
    "js-makedepend": "2.4.0",
    "mocha": "4.0.1",
    "npm-check-updates": "2.13.0",
    "nsp": "3.1.0",
    "pegjs": "0.10.0",
    "requirejs": "2.3.5",
    "viz.js": "1.8.0"
  },
  "bin": {
    "smcat": "bin/smcat",
    "sm-cat": "bin/smcat",
    "sm_cat": "bin/smcat",
    "state-machine-cat": "bin/smcat"
  },
  "dependencies": {
    "amdefine": "1.0.1",
    "commander": "2.11.0",
    "semver": "5.4.1"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://sverweij.gitlab.io/state-machine-cat/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sverweij/state-machine-cat"
  },
  "bugs": {
    "url": "https://github.com/sverweij/state-machine-cat/issues"
  }
}
