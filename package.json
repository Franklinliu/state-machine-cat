{
  "name": "state-machine-cat",
  "version": "1.1.9",
  "description": "smcat turns text into state charts",
  "main": "src/index.js",
  "scripts": {
    "depcruise": "depcruise -v -x node_modules -- src test",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "npm-check-updates": "ncu --upgrade",
    "nsp": "nsp check",
    "postversion": "git push && git push --tags && npm publish",
    "preversion": "test `git branch | grep \"^* [a-zA-Z]\" | cut -c 3-` = 'master'",
    "test": "mocha -R spec --recursive test",
    "test:cover": "istanbul cover node_modules/mocha/bin/_mocha -- --timeout 4000 --recursive test"
  },
  "keywords": [
    "state",
    "state chart",
    "state diagram",
    "state machine",
    "finite state machine",
    "fsm"
  ],
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "devDependencies": {
    "chai": "4.1.1",
    "chai-xml": "0.3.1",
    "dependency-cruiser": "2.3.2",
    "eslint": "4.5.0",
    "handlebars": "4.0.10",
    "istanbul": "0.4.5",
    "js-makedepend": "2.2.6",
    "mocha": "3.5.0",
    "npm-check-updates": "2.12.1",
    "nsp": "2.7.0",
    "pegjs": "0.10.0",
    "requirejs": "2.3.5",
    "viz.js": "1.8.0"
  },
  "bin": {
    "smcat": "bin/smcat",
    "sm-cat": "bin/smcat",
    "sm_cat": "bin/smcat",
    "state-machine-cat": "bin/smcat"
  },
  "dependencies": {
    "amdefine": "1.0.1",
    "commander": "2.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/sverweij/state-machine-cat.git"
  }
}
